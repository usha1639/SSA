<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.SSA974021" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>SSA</name>
  <description>
        An Ionic Framework and Cordova project.
    </description>
  <author email="mails2prashanth@gmail.com" href="http://example.com.com/">
      Prashanth Devaraj
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
